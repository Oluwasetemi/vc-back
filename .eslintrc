{
    "extends": ["wesbos", "plugin:import/errors", "plugin:import/warnings"],
    "rules": {
        "no-console": 2,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "all",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 4
            }
        ]
    },
    "settings": {
        "import/resolver": {
            "babel-module": {}
        }
    },
    "root": true
}
